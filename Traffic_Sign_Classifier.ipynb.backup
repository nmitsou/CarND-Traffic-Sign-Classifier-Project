    "from scipy import ndimage\n",
    "\n",
    "initial_size = len(X_train)\n",
    "#print(initial_size)\n",
    "\n",
    "hist, bin_edges = np.histogram(y_train, bins=list(range(0, n_classes+1)))\n",
    "max_class = max(hist)\n",
    "\n",
    "#print(y_train.shape)\n",
    "for i in bin_edges:\n",
    "    if i == 43:\n",
    "        continue\n",
    "    print(\"Balancing class: \"+str(i))\n",
    "    if (hist[i]>max_class/4):\n",
    "        continue\n",
    "    else:\n",
    "        inverseIndex = np.where(y_train == i)\n",
    "        added = 0\n",
    "        for k in inverseIndex[0]:\n",
    "            #print(added+hist[i])\n",
    "            if (added + hist[i] > max_class/4):\n",
    "                break\n",
    "            for j in range(5):\n",
    "                added = added + 1\n",
    "                initial_img = X_train[k]\n",
    "                #img = transform_image(initial_img,20,10,5)\n",
    "\n",
    "                ang_rot = np.random.uniform(50)-25\n",
    "                img = ndimage.rotate(initial_img, np.random.uniform(ang_rot), reshape=False)\n",
    "\n",
    "                X_train = np.append(X_train, [img], axis=0)\n",
    "\n",
    "                plt.imshow(img)\n",
    "                y_train = np.append(y_train, [y_train[k]], axis=0)\n",
    "\n",
    "print(\"Done!\")                                                \n",
    "initial_size = len(X_train)\n",
    "#print(initial_size)"
